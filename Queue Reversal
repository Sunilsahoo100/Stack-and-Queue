//https://practice.geeksforgeeks.org/problems/queue-reversal/1#

queue<int> rev(queue<int> q)
{
    // add code here.
    
    vector<int> v;
    queue<int> ans;
    while(!q.empty())
    {
        v.push_back(q.front());
        q.pop();
    }
    for(int i=v.size()-1;i>=0;i--)
    {
        ans.push(v[i]);
    }
    return ans;
    
}
